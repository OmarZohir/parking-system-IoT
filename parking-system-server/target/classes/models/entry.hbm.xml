<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
 <hibernate-mapping>  
  <class name="com.iot.parking.models.Entry" table="entries">  
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
    <property name="spot_id"></property>            
    <property name="reservation_id"></property>      
    <property name="license_plate"></property>      
    <property name="enter_time"></property>  
    <property name="leave_time"></property>
    <property name="cost"></property>            
  </class>              
 </hibernate-mapping>